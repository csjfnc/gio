@{
    ViewBag.Title = "AjustByCidade";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section css {
    @Styles.Render("~/Content/Pace")
}

<div class="col-lg-12">
    @if (@ViewBag.Msg != null)
    {
        <div class="alert alert-success" style="margin:auto;width:95%;">            
            @ViewBag.Msg
        </div>
    }
    @if (@ViewBag.Error != null)
    {
        <div class="alert alert-danger" style="margin:auto;width:95%;">
            @ViewBag.Error
        </div>
    }
    <nav>
        <ul class="pager">
            <li>@Html.ActionLink("Voltar", "Index", "AjustePoligonos")</li>
            <li>@Html.ActionLink("Home", "Index", "Home")</li>
        </ul>
    </nav>
</div>