<!-- Modal fullscreen EditIP -->
<div class="modal modal-fullscreen fade" id="ModalEditIPFull" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="panel panel-primary margin-top-forms">
