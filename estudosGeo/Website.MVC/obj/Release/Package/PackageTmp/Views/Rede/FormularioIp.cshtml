                                <option value="PETALAS 10">PETALAS 10</option>
                                <option value="ESPECIAL">ESPECIAL</option>
                                <option value="">SEM INFORMACAO</option>
                            </select>
                        </div>
                        <div class="col-md-6">
                            <h5 class="text-primary">Tipo de Luminaria</h5>
                            <select id="tipoluminaria" class="form-control">
                                <option value="ABERTA">ABERTA</option>
                                <option value="FECHADA">FECHADA</option>
                                <option value="GRADE">GRADE</option>
                                <option value="REFLETOR">REFLETOR</option>
