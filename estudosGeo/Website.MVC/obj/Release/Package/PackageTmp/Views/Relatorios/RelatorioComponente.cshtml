        <div class="panel-heading text-center"><span class="glyphicon glyphicon-list" aria-hidden="true"></span> Cidades</div>
        <div class="panel-body">

            <div class="col-md-4">
                <div class="input-group input-group-md" id="selectCidadeInput">
                    <span class="input-group-addon"><b>CIDADE</b></span>
                    <select class="form-control" id="selectCidade" onchange="Global.get().OnChangeCidade()">
                        <option value="0">---</option>
                    </select>
                </div>
            </div>
            <div class="col-md-4">
                <div class="input-group input-group-md" id="selectOsInput">
                    <span class="input-group-addon"><b>ORDEM SERVIÇO</b></span>
                    <select class="form-control" id="selectOs" onchange="Global.get().verificaOs()">
                        <option value="0">---</option>
                    </select>
                </div>
            </div>
