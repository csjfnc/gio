<div id="div_barra_carregamento">
    <div class="progress">
        <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%">
            CARREGANDO...
        </div>
    </div>
</div>
<div class=" col-md-12 alert alert-danger" role="alert" id="erroCamposRel" style="display: none; margin: 0px;">
