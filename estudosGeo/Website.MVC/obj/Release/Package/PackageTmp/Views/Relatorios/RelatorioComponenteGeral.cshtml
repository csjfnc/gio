                <div class="input-group input-group-md" id="selecDataOrdemServico">
                    <span class="input-group-addon"><b>DATA PUBLICADO</b></span>
                    <select class="form-control" id="selectIDOrdemServico" onchange="Global.get().OnChangeDataPublicacao()">
                        <option value="0">---</option>
                    </select>
                </div>
            </div>
            <div class="col-md-4">
                <div class="input-group input-group-md" id="selectOsInput">
                    <span class="input-group-addon"><b>ORDEM SERVIÇO</b></span>
                    <select class="form-control" id="selectOs" onchange="Global.get().verificaOs()">
                        <option value="0">---</option>
                    </select>
                </div>
            </div>            
            <div class="col-md-4">
                <button type="submit" class="btn btn-primary btn-block" onclick="Global.get().CriaRelatorioGeral()"><span class="glyphicon glyphicon-ok-sign" aria-hidden="true"></span> Carregar</button>
