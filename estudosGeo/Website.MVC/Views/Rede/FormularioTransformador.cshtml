<!-- Modal fullscreen EditIP -->
<div class="modal modal-fullscreen fade" id="ModalFormularioTransformador" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="panel panel-primary margin-top-forms">
                <div class="panel-body">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h3 class="text-center text-primary"><span class="glyphicon glyphicon-th" aria-hidden="true"></span> Transformador</h3>
                    <div class="col-md-6 margin-top-forms">
                        <div class="col-md-6">
                            <h5 class="text-primary">Identificador Transformador</h5>
                            <input type="text" class="form-control" id="id_transformador" disabled />
                        </div>
                        <div class="col-md-6">
                            <h5 class="text-primary">Status</h5>
                            <select id="status_transformador" class="form-control">
                                <option value="EXISTENTE">EXISTENTE</option>
                                <option value="NAO ENCONTRADO">NAO ENCONTRADO</option>
                                <option value="">SEM INFORMACAO</option>
                            </select>
                        </div>
                        <div class="col-md-6">
                            <h5 class="text-primary">Proprietário</h5>
                            <select id="proprietario_transformador" class="form-control">
                                <option value="PROPRIO">PROPRIO</option>
                                <option value="PARTICULAR">PARTICULAR</option>
                                <option value="">SEM INFORMACAO</option>
                            </select>
                        </div>
                        <div class="col-md-6">
                            <h5 class="text-primary">Numero de Campo</h5>
                            <input type="text" class="form-control" id="numero_campo_transformador"/>
                        </div>
                        <div class="col-md-6">
                            <h5 class="text-primary">Potência Total (KVA)</h5>
                            <select id="potencia_total_transformador" class="form-control">
                                <option value="10">10 KVA</option>
                                <option value="15">15 KVA</option>
                                <option value="25">25 KVA</option>
                                <option value="30">30 KVA</option>
                                <option value="37.5">37.5 KVA</option>
                                <option value="45">45 KVA</option>
                                <option value="50">50 KVA</option>
                                <option value="75">75 KVA</option>
                                <option value="100">100 KVA</option>
                                <option value="112.5">112.5 KVA</option>
                                <option value="150">150 KVA</option>
                                <option value="225">225 KVA</option>
                                <option value="300">300 KVA</option>
                            </select>
                        </div>
                        <div class="col-md-6">
                            <h5 class="text-primary">Tipo de Ligação</h5>
                            <select id="tipo_ligacao_transformador" class="form-control">
                                <option value="ESTRELA">ESTRELA</option>
                                <option value="DELTA/ABERTO">DELTA/ABERTO</option>
                                <option value="DELTA/FECHADO">DELTA/FECHADO</option>
                                <option value="LUZ FUT DELTA">LUZ FUT DELTA</option>
                                <option value="MONOF TOPO">MONOF TOPO</option>
                                <option value="">SEM INFORMACAO</option>
                            </select>
                        </div>
                        <div class="col-md-6">
                            <h5 class="text-primary">Tensão Nominal BT (V)</h5>
                            <select id="tensao_nominal_transformador" class="form-control">
                                <option value="220">220</option>
                                <option value="230">230</option>
                                <option value="240">240</option>
                                <option value="380">380</option>
                            </select>
                        </div>
                        <div class="col-md-6">
                            <h5 class="text-primary">Tipo de Instalação</h5>
                            <select id="tipo_instalacao_transformador" class="form-control">
                                <option value="POSTE">POSTE</option>
                                <option value="PEDESTAL">PEDESTAL</option>
                                <option value="CAMARA">CAMARA</option>
                                <option value="PLATAFORMA">PLATAFORMA</option>
                                <option value="CABINE">CABINE</option>
                                <option value="QUIOSQUE">CABINE</option>
                                <option value="CUBICULO BLINDADO">CUBICULO BLINDADO</option>
                                <option value="">SEM INFORMACAO</option>
                            </select>
                        </div>
                        <div class="col-md-6">
                            <h5 class="text-primary">Fase</h5>
                            <input class="form-control" type="text" id="fase_transformador" placeholder="A B C" style="text-transform: uppercase">
                        </div>
                        <div class="col-md-6">
                            <h5 class="text-primary">Corta - Circuito (A)</h5>
                            <select id="corta_circuito_transformador" class="form-control">
                                <option value="50">50</option>
                                <option value="100">100</option>
                                <option value="INEXISTENTE">INEXISTENTE</option>
                                <option value="">SEM INFORMACAO</option>
                            </select>
                        </div>
                        <div class="col-md-12">
                            <h5 class="text-primary">Patrimônio</h5>
                            <input class="form-control" type="text" id="patrimonio_transformador">
                        </div>
                        <div class="col-md-6">
                            <h5 class="text-primary">Numero de Série</h5>
                            <input class="form-control" type="text" id="num_serie_transformador">
                        </div>
                        <div class="col-md-6">
                            <h5 class="text-primary">Fabricante</h5>
                            <input class="form-control" type="text" id="fabricante_transformador">
                        </div>
                        <div class="col-md-6">
                            <h5 class="text-primary">Data Fabricação</h5>
                            <input class="form-control" type="date" id="data_fabricacao_transformador">
                        </div>
                        <div class="col-md-6">
                            <h5 class="text-primary">Codigo Instalação</h5>
                            <input class="form-control" type="text" id="cod_instalacao_transformador">
                        </div>
                        <div class="col-md-12">
                            <h5 class="text-primary">Observação</h5>
                            <input class="form-control" type="text" id="observacao_transformador">
                        </div>
                    </div>
                    <div class="col-md-6 margin-top-forms">
                        <div id="carousel" class="carousel slide" data-ride="carousel">
                            <ol class="carousel-indicators" id="ol-carousel"></ol>
                            <div class="carousel-inner" role="listbox" id="div-carousel"></div>
                            <a class="left carousel-control" href="#carousel" role="button" data-slide="prev">
                                <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                                <span class="sr-only">Previous</span>
                            </a>
                            <a class="right carousel-control" href="#carousel" role="button" data-slide="next">
                                <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                                <span class="sr-only">Next</span>
                            </a>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="col-md-3 margin-top-forms">
                            <button type="button" class="btn btn-primary btn-block" onclick="GlobalMaps.get().VoltarOpcoesPoste();"><span class="glyphicon glyphicon-backward" aria-hidden="true"></span> Voltar </button>
                        </div>
                        <div class="col-md-3 margin-top-forms">
                            <button type="submit" class="btn btn-success btn-block" onclick="GlobalMaps.get().SalvarIP();"><span class="glyphicon glyphicon-floppy-saved" aria-hidden="true"></span> Salvar</button>
                        </div>
                        <div class="col-md-3 margin-top-forms">
                            <button type="button" class="btn btn-danger btn-block" data-dismiss="modal"><span class="glyphicon glyphicon-floppy-remove" aria-hidden="true"></span> Cancelar</button>
                        </div>
                        <div class="col-md-3 margin-top-forms">
                            <button type="submit" class="btn btn-info btn-block" onclick="GlobalMaps.get().AbreStreetView();"><span class="glyphicon glyphicon-globe" aria-hidden="true"></span> Abre Street</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
